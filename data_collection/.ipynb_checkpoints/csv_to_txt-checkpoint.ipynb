    "import pandas as pd\n",
    "\n",
    "articles = pd.read_csv('articles.csv')\n",
    "text = \"\"\n",
    "\n",
    "text += \"Number of articles: \" + str(articles.shape[0]) + \"\\n\\n\\n\"\n",
    "for b in articles['body']:\n",
    "    text += b\n",
    "with open(\"articles_spring_2019.txt\", \"w\") as text_file:\n",
    "    text_file.write(text)"
